{
  "name": "@pulumi-helpers/component-acme-cert",
  "version": "0.3.0",
  "keywords": [
    "pulumi",
    "pulumi-component",
    "acme-cert"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bingtsingw/pulumi-helpers"
  },
  "license": "MIT",
  "author": {
    "name": "bingtsingw",
    "email": "bingtsingw@gmail.com",
    "url": "https://github.com/bingtsingw"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check-type": "tsc --noEmit",
    "dev": "tsup --watch",
    "lint:js": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --cache --ext .js,.jsx,.ts,.tsx --fix",
    "test": "bun test"
  },
  "eslintConfig": {
    "extends": "@xstools-dev/eslint-config/base"
  },
  "dependencies": {
    "@pulumi/tls": "^5.0.9",
    "@pulumiverse/acme": "^0.6.1"
  },
  "devDependencies": {
    "@pulumi/pulumi": "^3.143.0"
  },
  "peerDependencies": {
    "@pulumi/pulumi": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs"
    ],
    "dts": true,
    "clean": true,
    "treeshake": true,
    "minify": true
  }
}
