{
  "name": "@pulumi-helpers/component-random-suffix",
  "version": "0.3.1",
  "keywords": [
    "pulumi",
    "pulumi-component",
    "random-suffix"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bingtsingw/pulumi-helpers"
  },
  "license": "MIT",
  "author": {
    "name": "bingtsingw",
    "email": "bingtsingw@gmail.com",
    "url": "https://github.com/bingtsingw"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check-type": "tsc --noEmit",
    "dev": "tsup --watch",
    "lint:js": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --cache --ext .js,.jsx,.ts,.tsx --fix",
    "test": "bun test"
  },
  "eslintConfig": {
    "extends": "@xstools-dev/eslint-config/base",
    "rules": {
      "no-new": "off"
    }
  },
  "dependencies": {
    "@pulumi/random": "^4.16.8"
  },
  "devDependencies": {
    "@pulumi/pulumi": "^3.144.1"
  },
  "peerDependencies": {
    "@pulumi/pulumi": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs"
    ],
    "dts": true,
    "clean": true,
    "treeshake": true,
    "minify": true
  }
}
